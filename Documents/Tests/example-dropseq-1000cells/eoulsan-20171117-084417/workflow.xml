<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<analysis>
  <formatversion>1.0</formatversion>
  <name>DropSeq workflow</name>
  <description>First Eoulsan workflow for DropSeq / 10x Genomics data</description>
  <author>Nathalie Lehmann</author>
  <steps>
    <step discardoutput="NO" id="umistepwhitelist7" skip="false">
      <name>umi_whitelist_dev</name>
      <inputs/>
      <parameters/>
    </step>
    <step discardoutput="NO" id="umistepextract7" skip="false">
      <name>umi_extract</name>
      <inputs/>
      <parameters/>
    </step>
    <step discardoutput="NO" id="filterreads7" skip="false">
      <name>filterreads</name>
      <inputs/>
      <parameters>
        <parameter>
          <name>trim.length.threshold</name>
          <value>11</value>
        </parameter>
        <parameter>
          <name>quality.threshold</name>
          <value>12</value>
        </parameter>
      </parameters>
    </step>
    <step discardoutput="NO" id="mapreads7" skip="false">
      <name>mapreads</name>
      <inputs/>
      <parameters>
        <parameter>
          <name>mapper</name>
          <value>star</value>
        </parameter>
      </parameters>
    </step>
    <step discardoutput="NO" id="filtersam7" skip="false">
      <name>filtersam</name>
      <inputs/>
      <parameters>
        <parameter>
          <name>removeunmapped</name>
        </parameter>
        <parameter>
          <name>removemultimatches</name>
        </parameter>
      </parameters>
    </step>
    <step discardoutput="NO" id="countstep7" skip="false">
      <name>umi_count</name>
      <inputs/>
      <parameters/>
    </step>
  </steps>
  <globals>
    <parameter>
      <name>main.docker.uri</name>
      <value>unix:///var/run/docker.sock</value>
    </parameter>
    <parameter>
      <name>main.docker.mount.nfs.roots</name>
      <value>true</value>
    </parameter>
    <parameter>
      <name>main.galaxy.tool.path</name>
      <value>/kingdoms/csb/workspace3/lehmann/Eoulsan/galaxy/galaxytools</value>
    </parameter>
    <parameter>
      <name>main.format.path</name>
      <value>/kingdoms/csb/workspace3/lehmann/Eoulsan/galaxy/formats</value>
    </parameter>
  </globals>
</analysis>
